{
  "source": {
    "directories": [
      "lib"
    ]
  },
  "timeout": 4,
  "logs": {
    "text": "var/infection.log"
  },
  "phpUnit": {
    "configDir": ".",
    "customPath": "vendor/bin/phpunit"
  },
  "mutators": {
    "@return_value": true,
    "@sort": true,
    "@unwrap": true,
    "@zero_iteration": true,
    "RoundingFamily": true
  },
  "minMsi": 50,
  "minCoveredMsi": 80
}
