{
  "name": "rmp-up/wp-di",
  "license": "proprietary",
  "description": "Simple Dependency Injection / Service Container for more OOP in WordPress (PSR-11 compatible)",
  "autoload": {
    "psr-4": {
      "RmpUp\\WpDi\\": "lib"
    }
  },
  "require": {
    "johnpbloch/wordpress": "5.2.*",
    "pimple/pimple": "^3.0",
    "psr/container": "1.*"
  },
  "require-dev": {
    "php": ">=7.1",
    "phpstan/phpstan": "0.11.*",
    "phpunit/phpunit": "7.5.*|8.2.*",
    "pretzlaw/phpunit-docgen": "3.0.*",
    "pretzlaw/wp-integration-test": "0.2.*",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "3.4.*",
    "wp-cli/config-command": "2.0.*",
    "wp-cli/core-command": "2.0.*",
    "wp-cli/wp-cli": "2.2.*"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "wordpress-install-dir": "srv"
  }
}
